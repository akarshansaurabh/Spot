                        double func_val1 = 1.0 / 6.0 * (2 * pow(this->ai(joint_index, 0), 3) * this->am(joint_index, 0) + 3 * pow(this->am(joint_index, 0), 2) * this->jm(joint_index, 0) * this->v_target(joint_index, 0) + 3 * pow(this->jm(joint_index, 0), 2) * pow(this->v_target(joint_index, 0), 2) + 6 * this->ai(joint_index, 0) * this->am(joint_index, 0) * this->jm(joint_index, 0) * this->vi(joint_index, 0) - 3 * pow(this->am(joint_index, 0), 2) * this->jm(joint_index, 0) * val - 3 * pow(this->jm(joint_index, 0), 2) * pow(val, 2) + 6 * this->am(joint_index, 0) * pow(this->jm(joint_index, 0), 2) * this->xi(joint_index, 0) - 6 * pow((1.0 / 2.0 * pow(this->ai(joint_index, 0), 2) + this->jm(joint_index, 0) * this->vi(joint_index, 0) + this->jm(joint_index, 0) * val), 1.5) * this->am(joint_index, 0) + 6 * (pow(this->ai(joint_index, 0), 2) * this->am(joint_index, 0) + 2 * this->am(joint_index, 0) * this->jm(joint_index, 0) * this->vi(joint_index, 0)) * sqrt(1.0 / 2.0 * pow(this->ai(joint_index, 0), 2) + this->jm(joint_index, 0) * this->vi(joint_index, 0) + this->jm(joint_index, 0) * val)) / (this->am(joint_index, 0) * pow(this->jm(joint_index, 0), 2)) - this->x_target(joint_index, 0);
